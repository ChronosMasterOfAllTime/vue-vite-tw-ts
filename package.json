    "@vitest/ui": "^0.8.3",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/test-utils": "^2.0.0-rc.17",
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.0",
    "concurrently": "^7.1.0",
    "cypress": "^9.5.3",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-vue": "^8.5.0",
    "happy-dom": "^2.41.0",
