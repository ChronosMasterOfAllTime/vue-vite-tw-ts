{
  "name": "vue-ts-vite-esc",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run validate:types && vite build",
    "validate:types": "vue-tsc --noEmit",
    "serve": "vite preview",
    "test": "vitest run --silent",
    "test:watch": "vitest",
    "test:e2e:wsl": "env LIBGL_ALWAYS_INDIRECT=1 DISPLAY=:0 npm run test:e2e",
    "test:e2e": "env CYPRESS_TEST=true concurrently -r \"npm run dev\" \"cypress open\"",
    "test:e2e:headless": "env CYPRESS_TEST=true concurrently -k -r -s first \"npm run dev\" \"cypress run\"",
    "coverage": "npm run test -- --coverage",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@popperjs/core": "^2.11.8",
    "body-scroll-lock": "^4.0.0-beta.0",
    "pinia": "^2.3.1",
    "vue": "^3.4.23",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/types": "^7.26.8",
    "@badeball/cypress-cucumber-preprocessor": "^20.1.2",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/code-coverage": "^3.13.11",
    "@cypress/vite-dev-server": "^5.2.1",
    "@cypress/vue": "^6.0.2",
    "@pinia/testing": "^0.1.7",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20.17.17",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vicons/fa": "^0.12.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.0",
    "@vue/compiler-dom": "^3.4.23",
    "@vue/compiler-sfc": "^3.4.23",
    "@vue/server-renderer": "^3.4.33",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "c8": "^9.1.0",
    "concurrently": "^8.2.2",
    "cypress": "^13.17.0",
    "esbuild": "^0.20.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "happy-dom": "^17.0.4",
    "naive-ui": "^2.41.0",
    "postcss": "^8.5.2",
    "postcss-import": "^16.1.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.0",
    "sass": "^1.84.0",
    "stylelint": "^16.14.1",
    "stylelint-config-recommended": "^14.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vfonts": "^0.0.3",
    "vite": "^5.4.14",
    "vite-plugin-istanbul": "^6.0.2",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.2.0",
    "webpack": "^5.97.1"
  }
}
