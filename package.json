{
  "name": "vue-ts-vite-esc",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "npm run validate:types && vite build",
    "validate:types": "vue-tsc --noEmit",
    "serve": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e:wsl": "env LIBGL_ALWAYS_INDIRECT=1 DISPLAY=:0 npm run test:e2e",
    "test:e2e": "env CYPRESS_TEST=true concurrently -r \"npm run dev\" \"cypress open\"",
    "test:e2e:headless": "env CYPRESS_TEST=true concurrently -k -r -s first \"npm run dev\" \"cypress run\"",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.13",
    "@heroicons/vue": "^2.0.17",
    "@popperjs/core": "^2.11.7",
    "body-scroll-lock": "^4.0.0-beta.0",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/types": "^7.21.4",
    "@badeball/cypress-cucumber-preprocessor": "^18.0.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/vite-dev-server": "^5.0.5",
    "@cypress/vue": "^5.0.5",
    "@pinia/testing": "^0.0.16",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vicons/fa": "^0.12.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.31.1",
    "@vitest/ui": "^0.31.1",
    "@vue/compiler-dom": "^3.3.4",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/server-renderer": "^3.3.7",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.3.0",
    "c8": "^7.13.0",
    "concurrently": "^8.0.1",
    "cypress": "^13.3.0",
    "esbuild": "^0.17.18",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.11.0",
    "happy-dom": "^9.9.2",
    "naive-ui": "^2.34.3",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^13.0.0",
    "tailwindcss": "^3.3.1",
    "typescript": "^5.0.4",
    "vfonts": "^0.0.3",
    "vite": "^4.5.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.31.1",
    "vue-tsc": "^1.6.5",
    "webpack": "^5.80.0"
  }
}
