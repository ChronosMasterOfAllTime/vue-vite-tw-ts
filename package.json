{
  "name": "vue-ts-vite-esc",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run validate:types && vite build",
    "validate:types": "vue-tsc --noEmit",
    "serve": "vite preview",
    "test": "vitest run --silent",
    "test:watch": "vitest",
    "test:e2e:setup": "npm i -s && cypress cache clear && cypress install",
    "test:e2e:wsl": "env LIBGL_ALWAYS_INDIRECT=1 DISPLAY=:0 npm run test:e2e",
    "test:e2e": "env NODE_OPTIONS=\"--experimental-specifier-resolution=node\" CYPRESS_TEST=true concurrently -r \"npm run dev\" \"cypress open --project .\"",
    "test:e2e:headless": "env NODE_OPTIONS=\"--experimental-specifier-resolution=node\" CYPRESS_TEST=true concurrently -k -r -s first \"npm run dev\" \"cypress run --project .\"",
    "coverage": "npm run test -- --coverage",
    "lint": "eslint --fix src",
    "format": "prettier . --write"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@popperjs/core": "^2.11.8",
    "body-scroll-lock": "^4.0.0-beta.0",
    "globals": "^16.4.0",
    "pinia": "^3.0.2",
    "vue": "^3.4.23",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/types": "^7.27.0",
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.4",
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@cypress/code-coverage": "^3.14.0",
    "@cypress/vite-dev-server": "^6.0.3",
    "@cypress/vue": "^6.0.2",
    "@eslint/js": "^9.20.0",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/free-brands-svg-icons": "^7.0.0",
    "@fortawesome/free-regular-svg-icons": "^7.0.0",
    "@fortawesome/free-solid-svg-icons": "^7.0.0",
    "@fortawesome/vue-fontawesome": "^3.1.1",
    "@pinia/testing": "^1.0.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss":"^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/compiler-dom": "^3.4.23",
    "@vue/compiler-sfc": "^3.4.23",
    "@vue/server-renderer": "^3.5.22",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "c8": "^10.1.3",
    "concurrently": "^9.1.2",
    "cypress": "^14.3.0",
    "esbuild": "^0.25.2",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-vue": "^10.5.0",
    "happy-dom": "^20.0.2",
    "naive-ui": "^2.41.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-nested": "^7.0.2",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "sass": "^1.84.0",
    "stylelint": "^16.24.0",
    "stylelint-config-recommended": "^15.0.0",
    "tailwindcss": "^4.0.6",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vfonts": "^0.0.3",
    "vite": "^7.1.7",
    "vite-plugin-istanbul": "^7.1.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.0",
    "webpack": "^5.99.5"
  }
}
