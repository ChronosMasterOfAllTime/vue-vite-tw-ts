{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "types": ["cypress", "node"],
    "esModuleInterop": true,
    "noImplicitAny": false,
    "importHelpers": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node"
  },
  "include": ["**/*.ts", "**/*.d.ts", "node_modules/cypress"]
}
